\begin_layout Subsection
\begin_inset Formula $f\left(x,y\right)=x^{2}+y^{2}$
\end_inset

, 
\begin_inset Formula $v=\left(1,0\right)$
\end_inset

, 
\begin_inset Formula $\left(x_{0},y_{0}\right)=\left(1,2\right)$
\end_inset

 y 
\begin_inset Formula $v=\left(0,1\right)$
\end_inset

, 
\begin_inset Formula $\left(x_{0},y_{0}\right)=\left(1,2\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{t\to0}\frac{f(\left(1,2\right)+t(0,1))-f(1,2)}{t}=\lim_{t\to0}\frac{1+4+4t+t^{2}-1-4}{t}=\lim_{t\to0}\frac{2t+t^{2}}{t}=\lim_{t\to0}4+t=4
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $f(x,y)=2xy-3x^{2}+y-5$
\end_inset

, 
\begin_inset Formula $v=\left(1,1\right)$
\end_inset

, 
\begin_inset Formula $\left(x_{0},y_{0}\right)=\left(2,3\right)$
\end_inset

 y 
\begin_inset Formula $v=\left(1,2\right)$
\end_inset

, 
\begin_inset Formula $\left(x_{0},y_{0}\right)=\left(0,1\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
v'=\left(\frac{1}{\sqrt{5}},\frac{2}{\sqrt{5}}\right)\rightarrow\mbox{normalizo el vector}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{t\to0}\frac{f\left(\left(0,1\right)+t\left(\frac{1}{\sqrt{5}},\frac{2}{\sqrt{5}}\right)\right)-f\left(0,1\right)}{t}=\lim_{t\to0}\frac{1+t\frac{2}{\sqrt{5}}-5-\left(1-5\right)}{t}=\lim_{t\to0}\frac{1+t\frac{2}{\sqrt{5}}-5-1+5}{t}=\lim_{t\to0}\frac{t2}{t\sqrt{5}}=\frac{2}{\sqrt{5}}
\]

\end_inset


\end_layout

