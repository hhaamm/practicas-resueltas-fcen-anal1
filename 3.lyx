\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f\mbox{ es derivable en }x=a\Leftrightarrow\exists\lim_{t\to0}\frac{f(x+t)-f(x)}{t}=m
\]

\end_inset


\end_layout

