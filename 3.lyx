\series bold
recta tangente
\series default
 a 
\begin_inset Formula $f(x)$
\end_inset

 en 
\begin_inset Formula $x=a$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{x\to a}\frac{f(x)-L(x)}{x-a}=0\Rightarrow L(x)\to f(a)\mbox{ si }x\to a
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{Sabemos que la función que mejor aproxima }f(x)\mbox{ es }f'(x)\Rightarrow L(x)=f'(x)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
