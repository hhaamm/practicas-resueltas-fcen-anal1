\end_inset

.
 Demostrar las siguientes afirmaciones:
\end_layout

\begin_layout Subsection
Si 
\begin_inset Formula $f'=g'$
\end_inset

 en 
\begin_inset Formula $\left(a,b\right)$
\end_inset

, entonces 
\begin_inset Formula $f(x)=g(x)+c$
\end_inset

 donde 
\begin_inset Formula $c$
\end_inset

 es una constante.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f'=g'\Leftrightarrow D(f(x)+c')=D(g(x)+c'')\Rightarrow f(x)+c'=g(x)+c''\Leftrightarrow f(x)=g(x)+c''-c'
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mbox{sea }c=c''-c'\Rightarrow f(x)=g(x)+c
\]

\end_inset


\end_layout

\begin_layout Subsection
Si 
\begin_inset Formula $f'(x)>0\forall x\in\left(a,b\right)$
\end_inset

 , entonces 
\begin_inset Formula $f$
\end_inset

 es estrictamente creciente.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f'(x)>0\forall x\in\left(a,b\right)\Leftrightarrow\lim_{t\to0}\frac{f(x+t)-f(x)}{t}>0\Leftrightarrow f(x+t)-f(x)>t\Leftrightarrow f(x+t)-t>f(x)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f(x+t)-t>f(x)\rightarrow f(x+t)\mbox{ es mayor a }f(x)\mbox{ para cualquier }t\wedge x+t>t\Rightarrow f\mbox{ es estrictamente creciente.}
\]

\end_inset


\end_layout

\begin_layout Subsection
Si 
\begin_inset Formula $f'(x)<0\forall x\in\left(a,b\right)$
\end_inset

 , entonces 
\begin_inset Formula $f$
\end_inset

 es estrictamente decreciente.
\end_layout

