\begin_layout Section
Verificar que se cumple el teorema de Rolle para las siguientes funciones
 en los intervalos indicados:
\end_layout

\begin_layout Subsection
\begin_inset Formula $f(x)=x^{2}-3x+2$
\end_inset

 en el intervalo 
\begin_inset Formula $[1,2]$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f\mbox{ es continua en }\mathbb{R}\wedge f(1)=0=f(2)\wedge\left(f'(x)=2x-3=0\Leftrightarrow x=\frac{3}{2}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{3}{2}\in(1,2)\therefore\mbox{Se cumple el teorema de Rolle}
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $f(x)=(x-1)(x-2)(x-3)$
\end_inset

 en el intervalo 
\begin_inset Formula $[1,3]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f\mbox{ es continua en }\mathbb{R}\wedge f(1)=0=f(3)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f'(x)=D\left(\left(x^{2}-3x+2\right)\left(x-3\right)\right)=D\left(x^{3}-3x^{2}+2x-3x^{2}+9x-6\right)=D\left(x^{3}-6x^{2}+11x-6\right)=3x^{2}-12x+11
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f'(x)=0\Leftrightarrow x=2,577..\vee x=1,422...
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2,577...\in[1,3]\therefore\mbox{Se cumple el teorema de Rolle}
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $f(x)=\sin^{2}x$
\end_inset

 en el intervalo 
\begin_inset Formula $[0,\pi]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f\mbox{ es continua en }\mathbb{R}\wedge f(0)=0=f(\pi)\wedge\left(f'(x)=2\cos x=0\Leftrightarrow x=\frac{\pi}{2}+k\pi\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
k=0:f'(\frac{\pi}{2})=2\cos\frac{\pi}{2}=0\wedge\frac{\pi}{2}\in(0,\pi)\therefore\mbox{Se cumple el teorema de Rolle}
\]

\end_inset


\end_layout

